    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "tsc": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2"
  }
